# ZeroUI Cloud Local Development Environment Variables
# Copy this file to zeroui_cloud.local.env and customize as needed

# Postgres Product Plane
POSTGRES_PRODUCT_USER=zeroui
POSTGRES_PRODUCT_PASSWORD=zeroui_dev_only
POSTGRES_PRODUCT_DB=zeroui_product

# Postgres Shared Plane
POSTGRES_SHARED_USER=zeroui
POSTGRES_SHARED_PASSWORD=zeroui_dev_only
POSTGRES_SHARED_DB=zeroui_shared

# Redis
REDIS_PORT=6379

